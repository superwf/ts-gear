{
  "name": "ts-gear",
  "version": "2.0.0-alpha.10",
  "description": "swagger to typescript",
  "main": "lib/index.js",
  "bin": {
    "tsg": "lib/main.js",
    "ts-gear": "lib/main.js"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "coverage": "cross-env NODE_ENV=test jest --coverage | ts-node script/coverageReportToMarkdown.ts && jest-coverage-badges output ./badges && open-cli coverage/lcov-report/index.html -- google-chrome",
    "build": "cross-env rimraf lib && cross-env tsc -p tsconfig.build.json && chmod +x lib/main.js",
    "release": "standard-version",
    "prepublishOnly-": "yarn run-s test build release"
  },
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/superwf/ts-gear.git"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^2.4.2",
    "commander": "^4.1.0",
    "cosmiconfig": "^5.2.1",
    "fs-extra": "^7.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-html-reporter": "^3.1.0",
    "json-schema": "^0.2.3",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "path-to-regexp": "^3.0.0",
    "prettier": "^2.0.5",
    "prompts": "^2.3.2",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "translation.js": "^0.7.13",
    "traverse": "^0.6.6",
    "ts-morph": "^7.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.9.5",
    "url-join": "^4.0.1"
  },
  "files": [
    "src",
    "*.md",
    "*.json",
    "*.js",
    "lib"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/prompt-cli": "^8.1.0",
    "@types/chalk": "^2.2.0",
    "@types/cosmiconfig": "^5.0.3",
    "@types/fs-extra": "^5.0.5",
    "@types/http-server": "^0.10.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.12",
    "@types/json-schema": "^7.0.3",
    "@types/lodash": "^4.14.110",
    "@types/mkdirp": "^1.0.0",
    "@types/moxios": "^0.4.9",
    "@types/node": "^11.13.8",
    "@types/node-fetch": "^2.3.3",
    "@types/prettier": "^2.0.0",
    "@types/prompts": "^2.0.5",
    "@types/rimraf": "^2.0.3",
    "@types/swagger-schema-official": "^2.0.21",
    "@types/traverse": "^0.6.25-alpha",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "commitlint": "^8.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fetch-mock": "^9.4.0",
    "husky": "^3.0.2",
    "jest": "^25.4.0",
    "jest-coverage-badges": "^1.1.2",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "readline": "^1.3.0",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^25.4.0"
  },
  "keywords": [
    "openapi",
    "swagger",
    "typescript",
    "json schema",
    "ts-gear",
    "code generator",
    "interface",
    "structure"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "superwf",
  "license": "MIT"
}
