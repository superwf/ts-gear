{
  "name": "ts-gear",
  "version": "4.12.0",
  "description": "swagger to typescript with mock data",
  "main": "lib/index.js",
  "bin": {
    "tsg": "lib/main.js",
    "ts-gear": "lib/main.js"
  },
  "scripts": {
    "test": "jest",
    "coverage": "cross-env NODE_ENV=test jest --coverage | ts-node script/coverageReportToMarkdown.ts && jest-coverage-badges output ./badge && open-cli coverage/lcov-report/index.html -- google-chrome",
    "build": "cross-env rimraf lib && cross-env tsc -p tsconfig.build.json && chmod +x lib/main.js",
    "release": "standard-version",
    "prepublishOnly": "yarn run-s build test release",
    "publish-rc": "yarn test && yarn build && yarn standard-version -- prerelease rc && npm publish --ignore-scripts"
  },
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/superwf/ts-gear.git"
  },
  "peerDependencies": {
    "typescript": ">=4"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^8.0.0",
    "app-root-path": "^3.0.0",
    "chalk": "^2.4.2",
    "commander": "^8.2.0",
    "cosmiconfig": "^5.2.1",
    "cross-fetch": "^3.1.5",
    "fs-extra": "^7.0.1",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "openapi3-ts": "^1.4.0",
    "path-to-regexp": "^3.0.0",
    "prettier": "^2.2.1",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.2",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "traverse": "^0.6.6",
    "ts-morph": "^18.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "url-join": "^4.0.1"
  },
  "files": [
    "*.md",
    "lib"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "^13.2.0",
    "@commitlint/prompt-cli": "^13.2.1",
    "@types/app-root-path": "^1.2.4",
    "@types/cosmiconfig": "^5.0.3",
    "@types/fs-extra": "^5.0.5",
    "@types/http-server": "^0.10.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.110",
    "@types/mkdirp": "^1.0.0",
    "@types/moxios": "^0.4.9",
    "@types/node": "^11.13.8",
    "@types/prettier": "^2.2.0",
    "@types/prompts": "^2.0.5",
    "@types/rimraf": "^2.0.3",
    "@types/swagger-schema-official": "^2.0.21",
    "@types/traverse": "^0.6.25-alpha",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "axios": "^0.26.1",
    "commitlint": "^11.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fetch-mock": "^9.11.0",
    "husky": "^4.3.0",
    "jest": "^29.5.0",
    "jest-coverage-badges": "^1.1.2",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "readline": "^1.3.0",
    "standard-version": "^9.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "openapi",
    "swagger",
    "typescript",
    "json schema",
    "ts-gear",
    "code generator",
    "interface",
    "structure"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "superwf",
  "license": "MIT"
}
